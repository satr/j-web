<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN"
        "http://gwtproject.org/doctype/2.8.0/gwt-module.dtd">
<module rename-to="Main">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='io.github.satr.jweb.webshop.gwt.main.client.Main'/>


    <!-- Specify the app servlets.                   -->
    <servlet path='/MainService' class='io.github.satr.jweb.webshop.gwt.main.server.MainServiceImpl'/>
    <servlet path="/AccountService" class="io.github.satr.jweb.webshop.gwt.main.server.account.AccountServiceImpl"/>

</module>